{
	"manifest_version": 2,
	"name": "USD to TP Converter",
	"version": "0.1",
	"icons": { 
		"16": "tp_16.png",
    "48": "tp_48.png",
    "128": "tp_128.png" 
  },
  "permissions": ["activeTab"],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["jquery-3.4.1.min.js", "content.js"]
		}
	]
}
